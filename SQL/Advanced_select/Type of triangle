SELECT CASE
    -- Not a valid triangle (triangle inequality rule fails)
    WHEN A + B <= C OR A + C <= B OR B + C <= A THEN 'Not A Triangle'
    
    -- All three sides equal
    WHEN A = B AND B = C THEN 'Equilateral'
    
    -- Any two sides equal
    WHEN A = B OR B = C OR A = C THEN 'Isosceles'
    
    -- All sides different
    ELSE 'Scalene'
END AS TriangleType
FROM TRIANGLES;
